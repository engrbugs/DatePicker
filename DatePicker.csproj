<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Title>Date Picker</Title>
    <PackageIcon>icons8-calendar-11-ios-16-filled-96.png</PackageIcon>
    <ApplicationIcon>icons8-calendar-11-ios-16-filled-96.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icons8-calendar-11-ios-16-filled-96.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="icons8-calendar-11-ios-16-filled-96.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.5" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons8-calendar-11-ios-16-filled-96.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </Resource>
  </ItemGroup>

</Project>
